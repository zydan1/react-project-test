  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "connect": "^3.6.6",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "fs-extra": "^5.0.0",
    "multer": "^1.3.0",
    "react": "^16.3.2",
    "react-alert": "^4.0.2",
    "sync": "^0.2.5",
    "uuid": "^3.2.1"
  }
}
